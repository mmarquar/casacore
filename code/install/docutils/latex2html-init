#"\n" .
#"\n" .
"}\n" .
#"\n" .
"function imgOn(imgName) {\n" .
"        if (document.images) {\n" .
"            document[imgName].src = eval(imgName + \"on.src\");        }}\n" .
#"                        \n" .
"function imgOff(imgName) {\n" .
"        if (document.images) {\n" .
"            document[imgName].src = eval(imgName + \"off.src\");        }}\n" .
#"\n" .
"//-->\n" .
#"\n" .
"</SCRIPT>\n" .
"\n"
}
sub aips2topbar {
#"\n" .
"<FONT FACE=\"Arial,Helvetica\">\n" .
"<table cellpadding=0 cellspacing=0 border=0>\n" .
"<tr><td>\n" .
"<A HREF=\"$DOCSROOT/html/navpages/gettingstarted6.html\" \n" .
"         onMouseOver=\"imgOn('img1')\" \n" .
"         onMouseOut=\"imgOff('img1')\">\n" .
"<IMG NAME=\"img1\" SRC=\"$DOCSROOT/html/navpages/images3/gettingstartednav6off.gif\" BORDER=0></A>\n" .
"</td><td>\n" .
"<A HREF=\"$DOCSROOT/html/navpages/documentation.html\" \n" .
"         onMouseOver=\"imgOn('img2')\" \n" .
"         onMouseOut=\"imgOff('img2')\">\n" .
"<IMG NAME=\"img2\" SRC=\"$DOCSROOT/html/navpages/images3/documentationnav6off.gif\" BORDER=0></A>\n" .
"</td><td>\n" .
"<A HREF=\"$DOCSROOT/html/navpages/learnmore6.html\" \n" .
"         onMouseOver=\"imgOn('img3')\" \n" .
"         onMouseOut=\"imgOff('img3')\">\n" .
"<IMG NAME=\"img3\" SRC=\"$DOCSROOT/html/navpages/images3/learnmorenav6off.gif\" BORDER=0></A>\n" .
"</td></tr><tr><td>\n" .
"<A HREF=\"$DOCSROOT/html/navpages/search/search.html\"         \n" .
"          onMouseOver=\"imgOn('img4')\" \n" .
"          onMouseOut=\"imgOff('img4')\">\n" .
"<IMG NAME=\"img4\" SRC=\"$DOCSROOT/html/navpages/images3/searchnav6off.gif\" BORDER=0></A>\n" .
"</td><td>\n" .
"<A HREF=\"$DOCSROOT/html/navpages/communicate6.html\" \n" .
"          onMouseOver=\"imgOn('img5')\" \n" .
"          onMouseOut=\"imgOff('img5')\">\n" .
"<IMG NAME=\"img5\" SRC=\"$DOCSROOT/html/navpages/images3/communicatenav6off.gif\" BORDER=0></A>\n" .
"</td><td>\n" .
"<A HREF=\"$DOCSROOT/html/aips++.html\" \n" .
"          onMouseOver=\"imgOn('img6')\" \n" .
"          onMouseOut=\"imgOff('img6')\">\n" .
"<IMG NAME=\"img6\" SRC=\"$DOCSROOT/html/navpages/images3/homenav6off.gif\" BORDER=0></A>\n" .
"\n" .
"</td></tr>\n" .
"</table>\n"
}
sub aips2botbar {
"\n" .
"<FONT FACE=\"Arial,Helvetica\">\n" .
"<table cellpadding=0 cellspacing=0 border=0>\n" .
"<tr><td>\n" .
"<A HREF=\"$DOCSROOT/html/navpages/gettingstartedon6.html\" \n" .
"         onMouseOver=\"imgOn('img1')\" \n" .
"         onMouseOut=\"imgOff('img1')\">\n" .
"<IMG NAME=\"img1\" SRC=\"$DOCSROOT/html/navpages/images3/gettingstartednav6off.gif\" BORDER=0></A>\n" .
"</td><td>\n" .
"<A HREF=\"$DOCSROOT/html/navpages/documentation.html\" \n" .
"         onMouseOver=\"imgOn('img2')\" \n" .
"         onMouseOut=\"imgOff('img2')\">\n" .
"<IMG NAME=\"img2\" SRC=\"$DOCSROOT/html/navpages/images3/documentationnav6off.gif\" BORDER=0></A>\n" .
"</td><td>\n" .
"<A HREF=\"$DOCSROOT/html/navpages/learnmore6.html\" \n" .
"         onMouseOver=\"imgOn('img3')\" \n" .
"         onMouseOut=\"imgOff('img3')\">\n" .
"<IMG NAME=\"img3\" SRC=\"$DOCSROOT/html/navpages/images3/learnnav6off.gif\" BORDER=0></A>\n" .
"</td></tr><tr><td>\n" .
